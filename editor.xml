<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="1.1" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="ObjectiveC" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="ADD_CARETS_ON_DOUBLE_CTRL" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledLanguages">
      <set>
        <option value="ObjectiveC" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme|checkme)\b.*" />
    <pattern useCustomColors="true" pattern="\b(not(a|e))\b.*">
      <option name="FOREGROUND" value="8c01e" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\b(DEBUG)\b.*">
      <option name="FOREGROUND" value="cd3300" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>